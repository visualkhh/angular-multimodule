{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
//      "@common/*": ["common/*"],
      "@app/*": ["src/app/*"],
      "@env/*": ["src/environments/*"],
      "@web-core/*": ["../../web-cores/web-core-angular/src/*"],
      "@web-core-app/*": ["../../web-cores/web-core-angular/src/app/*"],
      "@web-core-generate/*": ["../../web-cores/web-core-angular/generate/*"]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false,
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
